{
  "name": "flogo-timer-rand",
  "type": "flogo:trigger",
  "version": "0.9.0",
  "title": "Timer",
  "description": "Simple Timer Rand trigger",
  "homepage": "https://qingcloudhx/flow-plugin/tree/master/trigger/qingcloud-timer-rand",
  "handler": {
    "settings": [
      {
        "name": "startDelay",
        "type": "string",
        "description": "The start delay (ex. 1m, 1h, etc.), immediate if not specified"
      },
      {
        "name": "repeatInterval",
        "type": "string",
        "description": "The repeat interval (ex. 1m, 1h, etc.), doesn't repeat if not specified"
      },
      {
        "name": "deviceId",
        "type": "string",
        "required": true,
        "description": "device id"
      },
      {
        "name": "thingId",
        "type": "string",
        "required": true,
        "description": "thing id"
      },
      {
        "name": "device",
        "type": "array",
        "description": "The Setting Device array",
        "items": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string",
              "description": "desc prperty id"
            },
            "name": {
              "type": "string",
              "description": "desc prperty name"
            }
          }
        }
      }
    ]
  },
  "output": [
    {
      "name": "device",
      "type": "array",
      "description": "The Output Device array"
    },
    {
      "name": "deviceId",
      "type": "string",
      "required": true,
      "description": "device id"
    },
    {
      "name": "thingId",
      "type": "string",
      "required": true,
      "description": "thing id"
    }
  ]
}
